{
  "version": 2,
  "buildCommand": "npm install && cd backend && npm install && npm run build",
  "outputDirectory": "backend/dist",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
